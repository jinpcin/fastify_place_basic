    "pm2": "pm2 start src/index.ts --name place-basic --interpreter node --interpreter-args '--import tsx'",
    "pm2:stop": "pm2 stop place-basic",
    "pm2:restart": "pm2 restart place-basic",
    "pm2:delete": "pm2 delete place-basic",
    "pm2:logs": "pm2 logs place-basic",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Fastify + ioredis implementation of place-basic worker",
  "dependencies": {
    "fastify": "^5.6.2",
    "ioredis": "^5.8.2",
    "@fastify/cors": "^10.0.1",
    "@fastify/formbody": "^8.0.1",
    "dotenv": "^16.4.5",
    "undici": "^7.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
